<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>CafeAdmin</servlet-name>
        <servlet-class>CafeAdmin.CafeAdmin</servlet-class>
        <init-param>
            <param-name>adminID</param-name>
            <param-value>Integer</param-value>
        </init-param>
        <init-param>
            <param-name>adminUsername</param-name>
            <param-value>String</param-value>
        </init-param>
        <init-param>
            <param-name>adminPassword</param-name>
            <param-value>String</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>AddManagingStaff</servlet-name>
        <servlet-class>CafeAdmin.AddManagingStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteManagingStaff</servlet-name>
        <servlet-class>CafeAdmin.DeleteManagingStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchManagingStaff</servlet-name>
        <servlet-class>CafeAdmin.SearchManagingStaff</servlet-class>
        <init-param>
            <param-name>username</param-name>
            <param-value>String</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>CafeAdmin.SearchManagingStaff</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>CafeAdmin.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddStaffServlet</servlet-name>
        <servlet-class>CafeAdmin.AddStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManageStaffServlet</servlet-name>
        <servlet-class>CafeAdmin.ManageStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteStaffServlet</servlet-name>
        <servlet-class>CafeAdmin.DeleteStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchStaffServlet</servlet-name>
        <servlet-class>CafeAdmin.SearchStaffServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateManagingStaffServlet</servlet-name>
        <servlet-class>CafeAdmin.UpdateManagingStaffServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CafeAdmin</servlet-name>
        <url-pattern>/CafeAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddManagingStaff</servlet-name>
        <url-pattern>/AddManagingStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteManagingStaff</servlet-name>
        <url-pattern>/DeleteManagingStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchManagingStaff</servlet-name>
        <url-pattern>/SearchManagingStaff</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddStaffServlet</servlet-name>
        <url-pattern>/AddStaffServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManageStaffServlet</servlet-name>
        <url-pattern>/ManageStaffServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteStaffServlet</servlet-name>
        <url-pattern>/DeleteStaffServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchStaffServlet</servlet-name>
        <url-pattern>/SearchStaffServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateManagingStaffServlet</servlet-name>
        <url-pattern>/UpdateManagingStaffServlet</url-pattern>
    </servlet-mapping>
</web-app>
